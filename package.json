{
  "name": "electron-bondage-club",
  "version": "2.5.101",
  "description": "An electron wrapped bondge-club",
  "main": "./build/main.js",
  "author": "dynilath <dynilath@qq.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "dev": "npm run compile && electron .",
    "build": "npm run compile && electron-builder",
    "dist": "npm run compile && electron-builder --windows nsis:x64 --publish always"
  },
  "devDependencies": {
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.54.0",
    "replace-in-file": "^6.3.5",
    "typescript": "^5.3.2"
  },
  "build": {
    "appId": "bondage-club.electron-app",
    "extends": null,
    "icon": "./Logo.ico",
    "publish": "github",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "files": [
      "BondageClub/**",
      "build/**"
    ]
  },
  "dependencies": {
    "electron-settings": "^4.0.2",
    "sweetalert2": "^11.10.1"
  }
}
