{
  "name": "electron-bondage-club",
  "version": "2.8.107+f2",
  "description": "An electron wrapped bondge-club",
  "main": "./build/main.js",
  "author": "dynilath <dynilath@qq.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "dev": "npm run compile && electron .",
    "build": "npm run compile && electron-builder",
    "dist": "electron-builder --windows nsis:x64 --publish always"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/semver": "^7.5.8",
    "electron": "^29.0.1",
    "electron-builder": "24.12.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "bondage-club.electron-app",
    "extends": null,
    "icon": "./Logo.ico",
    "publish": "github",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "files": [
      "BondageClub/**",
      "build/**"
    ]
  },
  "dependencies": {
    "dexie": "^3.2.5",
    "electron-settings": "^4.0.2",
    "electron-updater": "6.1.8",
    "sweetalert2": "^11.10.5"
  }
}
