{
  "name": "electron-bondage-club",
  "version": "2.11.109-rc.2",
  "description": "An electron wrapped bondage-club",
  "main": "./build/main.js",
  "author": "dynilath <dynilath@qq.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "changelog": "npm run changelog-md && npm run changelog-html",
    "dev": "npm run compile && electron .",
    "build": "npm run compile && electron-builder",
    "dist": "electron-builder --windows nsis:x64 --publish always",
    "changelog-md": "conventional-changelog -p angular -r 5 -o CHANGELOG.md",
    "changelog-html": "node ./scripts/.changelog.js CHANGELOG.md dist/changelog.html"
  },
  "devDependencies": {
    "@electron-delta/builder": "^0.1.17",
    "@types/node": "^22.7.4",
    "@types/semver": "^7.5.8",
    "axios": "^1.7.7",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^32.1.2",
    "electron-builder": "25.1.7",
    "eslint": "^9.12.0",
    "github-markdown-css": "^5.7.0",
    "marked": "^14.1.3",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "bondage-club.electron-app",
    "extends": null,
    "icon": "Logo.ico",
    "afterAllArtifactBuild": ".electron-delta.js",
    "publish": "github",
    "asarUnpack": [
      "**/*.png",
      "**/*.jpg",
      "**/*.mp3"
    ],
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "allowElevation": true,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "files": [
      "BondageClub/**",
      "build/**"
    ]
  },
  "dependencies": {
    "@electron-delta/updater": "^0.1.17",
    "dexie": "^4.0.8",
    "electron-settings": "^4.0.2",
    "electron-updater": "6.3.9",
    "keytar": "^7.9.0",
    "sweetalert2": "^11.10.5"
  },
  "packageManager": "yarn@1.22.21"
}
